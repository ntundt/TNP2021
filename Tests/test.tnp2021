create integer function sqrt(integer x) {
    if (x < 0) {
        return(0);
    } elseif (x < 2) {
        return(x);
    };

    create integer small_cand;
    create integer large_cand;
    small_cand = sqrt(x / 4) * 2;
    large_cand = small_cand + 1;
    if (large_cand * large_cand > x) {
        return(small_cand);
    } else {
        return(large_cand);
    };
};

create proc function inc(integer a) {
    a = a + 1;
};

create integer fibFirstIteration;
create integer function fibonacci(integer a, integer b) {
    create integer c;
    c = a + b;

    if (fibFirstIteration) {
        output(toString(a));
        output(" ");
        output(toString(b));
        fibFirstIteration = 0;
    };

    output(" ");
    if (c < 1000) {
        output(toString(c));
        fibonacci(b, c);
    };

    return(0);
};

create integer function circleArea(integer radius) {
    create integer a;
    a = radius * radius * radius * 22 / 7 / radius;
    return(a);
};

create integer function f(integer x) {
    output(toString(x*x*x + x*x + x + 1));
    output(" ");
    return(0);
};

create integer function fpsin(integer x) {
    create integer c;
    create integer y;

    c = x << 17;
    x = x - 8192;
    
    x = x << 18;
    x = x >> 18;
    x = x*x >> 12;

    y = 19900 - (3516*x >> 14);
    y = 4096 - (x*y >> 16);

    if (c >= 0) {
        return(y);
    } else {
        return(0 - y);
    };
};

create integer function main() {
    output("1 - Fibonacci sequence\n2 - circle area\n3 - sine approximation\n4 - square root\n5 - increment");

    create integer choise;
    choise = inputInteger();
    
    if (choise == 1) {
        fibFirstIteration = 1;

        output("a: ");
        create integer a;
        a = inputInteger();
        
        output("b: ");
        create integer b;
        b = inputInteger();
        
        output("\n");
        fibonacci(a, b);
    } elseif (choise == 2) {
        output("r: ");
        output(toString(circleArea(inputInteger())));
    } elseif (choise == 3) {
        output("x: ");
        create integer x;
        x = inputInteger();
        output(toString(fpsin(x)));
    } elseif (choise == 4) {
        output("x: ");
        output(toString(sqrt(inputInteger())));
    } elseif (choise == 5) {
        create integer a;
        a = inputInteger();
        inc(a);
        output(toString(a));
    } else {
        output("Error");
        return(0 - 1);
    };

    return(0);
};

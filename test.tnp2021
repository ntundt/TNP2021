        firstIteration = 0;
    };

    output(" ");
    if (c < 1000) {
        output(toString(c));
        fibonacci(b, c);
    };

    return(0);
};

create integer function circleArea(integer radius) {
    create integer a;
    a = radius * radius * radius * 22 / 7 / radius;
    return(a);
};

create integer function f(integer x) {
    output(toString(x*x*x + x*x + x + 1));
    output(" ");
    return(0);
};

create integer function loopSimulation(integer iteration) {
    f(iteration);
    if (iteration < 15) {
        iteration = iteration + 1;
        loopSimulation(iteration);
    };
    return(0);
};

create integer function fpsin(integer x) {
    create integer c;
    create integer y;

    c = x << 17;
    x = x - 8192;
    
    x = x << 18;
    x = x >> 18;
    x = x*x >> 12;

    y = 19900 - (3516*x >> 14);
    y = 4096 - (x*y >> 16);

    if (c >= 0) {
        return(y);
    } else {
        return(0 - y);
    };
};

